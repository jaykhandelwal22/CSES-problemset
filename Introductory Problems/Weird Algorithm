#include<bits/stdc++.h>
using namespace std;
#define int long long int
//#define ll long long

int mod = 1000000007;

void init() {
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif

}



void solve()
{
  int n;
  cin>>n;
 // cout<<n<<" ";

  while(n>1)
  {
    cout<<n<<" ";
    if(n&1)
    {
        n=n*3+1;
    }
    else
    {
        n/=2;
    }

  }
  cout<<1;
  cout<<endl;

}
int32_t main()
{
    init();

    int tests = 1;
    //cin >> tests;
    for (int tt = 1; tt <= tests; tt++)
    {
        solve();
    }

    return 0;
}
